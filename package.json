{
  "name": "discord_playtime",
  "version": "0.1.1",
  "description": "Discord Bot that collects playtime statistics",
  "main": "index.js",
  "scripts": {
    "build": "babel app -d build --ignore __mocks__,__tests__",
    "test": "node_modules/jest/bin/jest.js",
    "test-watch": "node_modules/jest/bin/jest.js --watchAll",
    "start": "node index.js",
    "lint": "node_modules/eslint/bin/eslint.js --cache --format=node_modules/eslint-formatter-pretty .",
    "lint-fix": "npm run lint -- --fix",
    "flow": "flow",
    "flow-typed": "rimraf flow-typed/npm && flow-typed install --overwrite || true",
    "postinstall": "[ \"$NODE_ENV\" == production ] && exit 0; npm run flow-typed"
  },
  "author": "thiemok",
  "license": "GPL-3.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/thiemok/discord_playtime"
  },
  "devDependencies": {
    "babel-cli": "^6.24.1",
    "babel-eslint": "^7.2.3",
    "babel-plugin-flow-runtime": "^0.11.1",
    "babel-plugin-module-resolver": "^2.7.1",
    "eslint": "^4.3.0",
    "eslint-config-airbnb-base": "^11.3.1",
    "eslint-formatter-pretty": "^1.1.0",
    "eslint-plugin-flowtype": "^2.35.0",
    "eslint-plugin-flowtype-errors": "^3.3.1",
    "eslint-plugin-import": "^2.7.0",
    "flow-bin": "^0.52.0",
    "flow-runtime": "^0.14.0",
    "flow-typed": "^2.1.5",
    "jest": "^20.0.4",
    "jest-environment-node-debug": "^2.0.0",
    "mockdate": "^2.0.1"
  },
  "dependencies": {
    "async": "^2.5.0",
    "babel-plugin-add-module-exports": "^0.2.1",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-polyfill": "^6.23.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-flow": "^6.23.0",
    "babel-preset-stage-0": "^6.24.1",
    "babel-register": "^6.24.1",
    "discord.js": "^11.1.0",
    "express": "^4.15.3",
    "igdb-api-node": "^3.0.1",
    "mongodb": "^2.2.30"
  }
}
